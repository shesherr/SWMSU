{
  "name": "R Markdown",
  "scopeName": "text.html.markdown.rmarkdown",
  "patterns": [
    {
      "include": "#fenced_code_block_r"
    },
    {
      "include": "#fenced_code_block_c"
    },
    {
      "include": "#fenced_code_block_cpp"
    },
    {
      "include": "#fenced_code_block_yaml"
