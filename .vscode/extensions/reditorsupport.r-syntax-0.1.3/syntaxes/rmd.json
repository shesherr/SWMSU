    },
    {
      "include": "text.html.markdown"
    }
  ],
  "repository": {
    "fenced_code_chunk_options": {
      "patterns": [
        {
          "name": "variable.parameter.chunk-options.rmarkdown",
          "match": "(?:[a-zA-Z._][\\w.]*|`[^`]+`)(?=\\s*=[^=])"
        },
        {
          "begin": "(?==)",
          "end": "(?=[,\\}])",
          "patterns": [
            {
              "include": "source.r"
            }
          ]
        },
        {
          "name": "punctuation.separator.parameters.r",
          "match": ","
        },
        {
          "include": "source.r"
        }
      ]
    },
    "fenced_code_metadata": {
      "contentName": "meta.embedded.inline.r",
      "begin": "(\\{[a-zA-Z]+\\b)",
      "beginCaptures": {
        "1": {
          "name": "fenced_code.block.language.markdown.rmarkdown"
        }
      },
      "end": "(\\})",
      "patterns": [
        {
          "include": "#fenced_code_chunk_options"
        }
      ]
    },
    "fenced_code_block_c": {
      "name": "markup.fenced_code.block.markdown.rmarkdown",
      "begin": "(?:^)\\s*(`{3,})(?=\\{(?i:c|h)\\b)",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.markdown.rmarkdown"
        }
      },
      "end": "^\\s*(\\1)",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.markdown.rmarkdown"
        }
      },
      "patterns": [
        {
          "include": "#fenced_code_metadata"
        },
        {
          "contentName": "meta.embedded.block.c",
          "begin": "^",
          "while": "(^|\\G)(?!\\s*(`{3,})\\s*$)",
          "patterns": [
            {
              "include": "source.c"
            }
          ]
        }
      ]
    },
    "fenced_code_block_cpp": {
